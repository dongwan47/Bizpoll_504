package classpart;

import flower.VioletDAO;
import flower.VioletDTO;

public class PracticeMain {

	public static void main(String[] args) {

		VioletDTO violDto = new VioletDTO();
		violDto.setName("이름");
		violDto.setColor("색깔");
		violDto.setNumberOfLeaves("꽃잎개수");
		violDto.setBloom("개화기");
		violDto.setPlantLength("수고");
		
		VioletDAO violDao = new VioletDAO();
		
		violDao.violetInfo(violDto);
		
	}
}
