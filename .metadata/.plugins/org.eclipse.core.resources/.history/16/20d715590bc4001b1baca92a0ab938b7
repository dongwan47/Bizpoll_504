package classpart;

public class PracticeMain {

	public static void main(String[] args) {

		PracticeDTO pracDto = new PracticeDTO();
		pracDto.setName("이름");
		pracDto.setColor("색깔");
		pracDto.setNumberOfLeaves("꽃잎개수");
		pracDto.setBloom("개화기");
		pracDto.setPlantLength("수고");

		PracticeDAO pracDao = new PracticeDAO();

		pracDao.practiceInfo(pracDto);
	}

}
